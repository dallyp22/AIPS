{
  "name": "aips-backend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc -p tsconfig.json && prisma generate",
    "start": "node dist/main.js",
    "postinstall": "prisma generate",
    "deploy": "prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "seed": "tsx src/seed.ts",
    "seed:production": "tsx src/seedProduction.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@prisma/client": "^5.17.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-jwt": "^8.5.1",
    "express-oauth-server": "^2.0.0",
    "fastify": "^4.28.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "prisma": "^5.17.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4"
  }
}